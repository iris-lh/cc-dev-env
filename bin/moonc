#!/bin/sh

exec '/usr/local/bin/lua' -e 'package.path="/Users/isu/.luarocks/share/lua/5.2/?.lua;/Users/isu/.luarocks/share/lua/5.2/?/init.lua;/usr/local/share/lua/5.2/?.lua;/usr/local/share/lua/5.2/?/init.lua;"..package.path; package.cpath="/Users/isu/.luarocks/lib/lua/5.2/?.so;/usr/local/lib/lua/5.2/?.so;"..package.cpath' -e 'local k,l,_=pcall(require,"luarocks.loader") _=k and l.add_context("moonscript","0.4.0-1")' '/usr/local/lib/luarocks/rocks-5.2/moonscript/0.4.0-1/bin/moonc' "$@"
